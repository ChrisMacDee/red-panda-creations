<svg xmlns="http://www.w3.org/2000/svg" width="1200" height="675" viewBox="0 0 1200 675">
  <defs>
    <linearGradient id="reactBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#20232a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#282c34;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="reactGlow" cx="50%" cy="50%">
      <stop offset="0%" style="stop-color:#61dafb;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#61dafb;stop-opacity:0" />
    </radialGradient>
  </defs>

  <!-- Background -->
  <rect width="1200" height="675" fill="url(#reactBg)"/>
  <circle cx="600" cy="337.5" r="400" fill="url(#reactGlow)"/>

  <!-- React Atom Symbol -->
  <g opacity="0.6" stroke="#61dafb" stroke-width="2.5" fill="none">
    <!-- Ellipse 1 -->
    <ellipse cx="600" cy="337.5" rx="200" ry="80" transform="rotate(0 600 337.5)"/>
    <!-- Ellipse 2 -->
    <ellipse cx="600" cy="337.5" rx="200" ry="80" transform="rotate(60 600 337.5)"/>
    <!-- Ellipse 3 -->
    <ellipse cx="600" cy="337.5" rx="200" ry="80" transform="rotate(120 600 337.5)"/>
    <!-- Center dot -->
    <circle cx="600" cy="337.5" r="12" fill="#61dafb"/>
  </g>

  <!-- Code snippets -->
  <g opacity="0.15" fill="#61dafb" font-family="monospace" font-size="16">
    <text x="100" y="100">const [state, setState] = useState()</text>
    <text x="100" y="550">useEffect(() =&gt; {'{...}'}, [])</text>
    <text x="850" y="100">useMemo(() =&gt; {'{...}'}, [deps])</text>
    <text x="850" y="550">useCallback(fn, [deps])</text>
  </g>

  <!-- Title -->
  <text x="600" y="500" font-family="Arial, sans-serif" font-size="48" fill="#61dafb" text-anchor="middle" font-weight="bold">React Hooks</text>
  <text x="600" y="550" font-family="Arial, sans-serif" font-size="28" fill="rgba(97,218,251,0.7)" text-anchor="middle">Modern React Development</text>
</svg>
