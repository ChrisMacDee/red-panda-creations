<svg xmlns="http://www.w3.org/2000/svg" width="1200" height="675" viewBox="0 0 1200 675">
  <defs>
    <linearGradient id="leatherBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8b4513;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#654321;stop-opacity:1" />
    </linearGradient>
    <pattern id="leather" width="20" height="20" patternUnits="userSpaceOnUse">
      <circle cx="10" cy="10" r="1" fill="rgba(0,0,0,0.1)"/>
    </pattern>
  </defs>

  <!-- Background -->
  <rect width="1200" height="675" fill="url(#leatherBg)"/>
  <rect width="1200" height="675" fill="url(#leather)"/>

  <!-- Stitching Effect -->
  <path d="M 100 100 L 1100 100" stroke="rgba(255,255,255,0.3)" stroke-width="2" stroke-dasharray="10,15"/>
  <path d="M 100 575 L 1100 575" stroke="rgba(255,255,255,0.3)" stroke-width="2" stroke-dasharray="10,15"/>
  <path d="M 100 100 L 100 575" stroke="rgba(255,255,255,0.3)" stroke-width="2" stroke-dasharray="10,15"/>
  <path d="M 1100 100 L 1100 575" stroke="rgba(255,255,255,0.3)" stroke-width="2" stroke-dasharray="10,15"/>

  <!-- Tool Silhouettes -->
  <g opacity="0.3" fill="rgba(255,255,255,0.5)" stroke="rgba(255,255,255,0.7)" stroke-width="3">
    <!-- Hammer -->
    <rect x="200" y="250" width="15" height="150" rx="3"/>
    <rect x="180" y="230" width="55" height="40" rx="5"/>

    <!-- Awl -->
    <path d="M 400 250 L 405 400 L 395 400 Z"/>
    <rect x="390" y="240" width="20" height="30" rx="2"/>

    <!-- Ruler -->
    <rect x="550" y="280" width="200" height="25" fill="none" stroke="rgba(255,255,255,0.7)" stroke-width="3"/>
    <line x1="550" y1="280" x2="550" y2="305" stroke="rgba(255,255,255,0.5)" stroke-width="1.5"/>
    <line x1="600" y1="280" x2="600" y2="305" stroke="rgba(255,255,255,0.5)" stroke-width="1.5"/>
    <line x1="650" y1="280" x2="650" y2="305" stroke="rgba(255,255,255,0.5)" stroke-width="1.5"/>
    <line x1="700" y1="280" x2="700" y2="305" stroke="rgba(255,255,255,0.5)" stroke-width="1.5"/>
    <line x1="750" y1="280" x2="750" y2="305" stroke="rgba(255,255,255,0.5)" stroke-width="1.5"/>

    <!-- Knife -->
    <path d="M 900 350 L 950 250 L 955 250 L 905 350 Z"/>
    <rect x="895" y="350" width="15" height="80" rx="3"/>
  </g>

  <!-- Title -->
  <text x="600" y="500" font-family="Arial, sans-serif" font-size="48" fill="#f4e4c1" text-anchor="middle" font-weight="bold">Leather Crafting</text>
  <text x="600" y="550" font-family="Arial, sans-serif" font-size="28" fill="rgba(244,228,193,0.8)" text-anchor="middle">Tools &amp; Techniques</text>
</svg>
